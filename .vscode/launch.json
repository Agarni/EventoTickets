{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Executar EventoTickets.Server",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/EventoTickets/Server/bin/Debug/net9.0/EventoTickets.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/EventoTickets/Server",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "Now listening on: (https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/EventoTickets/Server/Views"
            }
        }
    ]
}